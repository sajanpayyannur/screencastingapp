<ul>
  <li><%= link_to (image_tag('google.png') + " Sign in with Google"), user_omniauth_authorize_path(:google) %> </li>
  <li><%= link_to (image_tag('facebook.png') + " Sign in with Facebook"), user_omniauth_authorize_path(:facebook) %></li>
  <li><%= link_to (image_tag('twitter.png') + " Sign in with Twitter"), user_omniauth_authorize_path(:twitter) %></li>
  <li><%= link_to (image_tag('openid.png') + " Sign in with Open Id"), user_omniauth_authorize_path(:open_id) %></li>
</ul>

<h2>Sign in</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <div><%= f.label :email %>
      <br/>
      <%= f.email_field :email %></div>

    <div><%= f.label :password %>
      <br/>
      <%= f.password_field :password %></div>

    <% if devise_mapping.rememberable? -%>
        <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
    <% end -%>

    <div><%= f.submit "Sign in" %></div>
<% end %>

<%= render "devise/shared/links" %>
